<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="wrap_content">

    <TextView
            android:id="@+id/seasonTextView"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="1"
            android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    <TextView
            android:text="88"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/assistsTextView" app:layout_constraintStart_toEndOf="@+id/goalsScoredTextView"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="666"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/goalConcededTextView" app:layout_constraintStart_toEndOf="@+id/cleanSheetsTextView"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="77"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/cleanSheetsTextView" app:layout_constraintStart_toEndOf="@+id/assistsTextView"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="99"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/goalsScoredTextView" app:layout_constraintStart_toEndOf="@+id/minutesPlayedTextView"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="9999"
            android:layout_width="38dp"
            android:layout_height="20dp"
            android:id="@+id/minutesPlayedTextView" app:layout_constraintStart_toEndOf="@+id/pointsTextView"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            android:gravity="center" android:textAlignment="center"/>

    <TextView
            android:id="@+id/pointsTextView"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="899"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/endPriceTextView"
            app:layout_constraintTop_toTopOf="parent" />

    <TextView
            android:text="55"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/pennaltiesSavedTextView" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/goalConcededTextView"
            android:layout_marginStart="8dp" android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="44"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/yellowCardsTextView" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/pennaltiesSavedTextView"
            android:layout_marginStart="8dp" android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="33"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/redCardsTextView" app:layout_constraintStart_toEndOf="@+id/yellowCardsTextView"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="222"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/savesTextView" app:layout_constraintStart_toEndOf="@+id/redCardsTextView"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="11"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:id="@+id/bonusTextView" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/savesTextView"
            android:layout_marginStart="8dp" android:gravity="center" android:textAlignment="center"/>
    <TextView
            android:text="00.0"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/startPriceTextView" app:layout_constraintStart_toEndOf="@+id/bonusTextView"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:gravity="center" android:textAlignment="center"/>

    <TextView
            android:id="@+id/endPriceTextView"
            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="SOU (H) 3-0"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/seasonTextView"
            app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>